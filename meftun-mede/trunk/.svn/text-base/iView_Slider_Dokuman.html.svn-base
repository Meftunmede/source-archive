html,body,div,h1,h2,h3,h4,a,code,ol,ul,li,table,tbody,thead,tr,th,td{border:0;font-weight:inherit;font-style:inherit;font-family:"Open Sans","lucida grande",sans-serif,sans-serif;vertical-align:baseline;margin:0;padding:0;}
