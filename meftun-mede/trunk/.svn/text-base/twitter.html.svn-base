function repetido(num) {
var repe = false;
for (i=0; i<usados.length; i++) {
if (num == usados[i]) {
repe = true;
}
}
return repe;
}
function aleatorio(min, max) {
if (usados.length<=(max-min)) {
while (repe != false) {
var num = Math.floor(Math.random()*(max-min+1))+min;
var repe = repetido(num);
}
usados.push(num);
return num;
} else {
return null;
}
}
